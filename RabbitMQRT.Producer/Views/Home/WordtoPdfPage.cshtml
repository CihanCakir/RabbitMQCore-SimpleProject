@model WordToPdf

@{
    ViewData["Title"] = "WordtoPdfPage";
}

<h1>WordtoPdfPage</h1>


<div class="row">
    <div class="col-md-4">
        <form asp-action="WordtoPdfPage" method="post" enctype="multipart/form-data">
            <div class="form-group">
                <label asp-for="File" class="control-label">                </label>

                <input asp-for="File" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Email" class="control-label">                </label>

                <input asp-for="Email" class="form-control" />
            </div>
            <div class="form-group">
                    <input asp-for="File" type="button" class="btn btn-danger" value="Converter" />
            </div>
        </form>
    </div>
</div>


